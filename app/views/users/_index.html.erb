
<h1>Users</h1>
<table>
  <tr><th> image </th><th> name </th></tr>
  <% @users.each do |user| %>
    <div>
        <td><span class="book-title"><p><%= image_tag user.get_profile_image(100,100) %></p></td>
        <td><span class="book-title"><p><p><%= user.name %></p></td>
        <td><span class="book-title"><p><%= link_to "Show", user_path(user.id) %></p></td>
    </div>
<table>

<% end %>